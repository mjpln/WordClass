                                                                    "                    wordids += ',';\r",
                                                                    "                    synonyms += ',';\r",
                                                                    "                }\r",
                                                                    "            }\r",
                                                                    "            Ext.Ajax.request({\r",
                                                                    "                url: '../synonym.action',\r",
                                                                    "                method: 'post',\r",
                                                                    "                params: {\r",
                                                                    "                    action: 'delete',\r",
                                                                    "                    stdwordid: wordids,\r",
                                                                    "                    synonym: synonyms,\r",
                                                                    "                    curworditem: curworditem,\r",
                                                                    "                    curwordclass: curwordclass\r",
                                                                    "                },\r",
                                                                    "                async: true,\r",
                                                                    "                success: function (result, request) {\r",
                                                                    "                    var retobj = Ext.JSON.decode(result.responseText);\r",
                                                                    "                    Ext.MessageBox.alert('批量删除提示',retobj.msg);\r",
                                                                    "                    if(retobj.success===true){\r",
                                                                    "                        var page = gridpanel.store.currentPage;\r",
                                                                    "                        if(page > 1){\r",
                                                                    "                            if(gridpanel.store.data.length<=1){\r",
                                                                    "                                page = page-1;\r",
                                                                    "                            }\r",
                                                                    "                        }\r",
                                                                    "                        gridpanel.store.loadPage(page);\r",
                                                                    "                    }\r",
                                                                    "                },\r",
                                                                    "                failure: function (result, request) {\r",
                                                                    "                    Ext.MessageBox.alert('系统异常', '请求数据失败！');\r",
                                                                    "                }\r",
                                                                    "           });\r",
                                                                    "        }else{\r",
                                                                    "            Ext.MessageBox.alert('提示','请至少选中一行');\r",
                                                                    "        }\r",
                                                                    "    }\r",
                                                                    "}"
                                                                ],
                                                                "name": "click",
                                                                "scope": "me"
                                                            },
                                                            "name": "onButtonClick7",
                                                            "designerId": "90af8abe-42d4-41c8-b201-f6b9f6d80ef4"
                                                        }
                                                    ]
                                                }
