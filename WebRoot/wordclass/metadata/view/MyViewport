                                                                    "        var gridpanel = Ext.getCmp(\"wordclassgridpanel\");\r",
                                                                    "        var wordclass = gridpanel.store.getAt(rowIndex).get('wordclass');\r",
                                                                    "        var wordclassid  = gridpanel.store.getAt(rowIndex).get('wordclassid');\r",
                                                                    "        var wordclasstype  = gridpanel.store.getAt(rowIndex).get('wordclasstype');\r",
                                                                    "        //alert(wordclasstype);\r",
