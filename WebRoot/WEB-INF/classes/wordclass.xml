<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="index1" namespace="/wordclass" extends="struts-default">
		<action name="index" class="com.knowology.km.action.WordclassAction"
			method="index"> 
			<result name="success">/wordclass/app.html</result>
		</action>
	</package>
	
	<package name="index2" namespace="/wordclass_sentence" extends="struts-default">
		<action name="index" class="com.knowology.km.action.WordclassSentenceAction"
			method="index"> 
			<result name="success">/wordclass_sentence/app.html</result>
		</action>
	</package>
	<package name="wordclass" namespace="/" extends="valid-default">
		<action name="wordclass" class="com.knowology.km.action.WordclassAction"
			method="execute">
			<interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="synonym" class="com.knowology.km.action.SynonymAction"
			method="execute">
			<interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="worditem" class="com.knowology.km.action.WorditemAction"
			method="execute">
			<interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>

		<action name="wordclasssentence" class="com.knowology.km.action.WordclassSentenceAction"
			method="execute">
			<interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="synonymsentence" class="com.knowology.km.action.SynonymSentenceAction"
			method="execute">
			<interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="worditemsentence" class="com.knowology.km.action.WorditemSentenceAction"
			method="execute">
			<interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
	</package>
		
	<package name="questionupload" namespace="/" extends="valid-default">
		<action name="getCityTree" class="com.knowology.km.action.QuestionUploadAction"
			method="getCityTree">
			 <interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="getAddCityTree" class="com.knowology.km.action.QuestionUploadAction"
			method="getAddCityTree">
			 <interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="getSelectCityTree" class="com.knowology.km.action.QuestionUploadAction"
			method="getSelectCityTree">
			 <interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="getSynonymCity" class="com.knowology.km.action.QuestionUploadAction"
			method="getSynonymCity">
			 <interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
		<action name="exportWord" class="com.knowology.km.action.QuestionUploadAction"
			method="exportWord">
			 <interceptor-ref name="validLoginStack"/> 
			<!--<result name="success" type="json">
				<param name="root">m_result</param>
			</result>-->
			<result name="export" type="stream">
				<param name="inputName">fileInputStream</param>
				<param name="contentDisposition">attachment;fileName="${fileName}"</param>
				<param name="contentType">application/vnd.ms-excel</param>
				<param name="bufferSize">2048</param>
			</result>
			<!--<result name="jsonstring" type="stream">
				<param name="inputName">fileInputStream</param>
				<param name="contentType">text/html</param>
			</result>-->
		</action>
			<action name="isShow" class="com.knowology.km.action.QuestionUploadAction"
			method="isShow">
			 <interceptor-ref name="validLoginStack"/> 
			<result name="success" type="json">
				<param name="root">m_result</param>
			</result>
		</action>
	</package>	
</struts>